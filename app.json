{
	"expo": {
		"name": "ThinkTok",
		"slug": "gear",
		"version": "1.0.17",
		"orientation": "portrait",
		"icon": "./assets/images/logo.png",
		"scheme": "thinktok",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.aakgna.gear",
			"googleServicesFile": "./GoogleService-Info.plist",
			"buildNumber": "1"
		},
		"android": {
			"versionCode": 1,
			"package": "com.aakgna.gear",
			"googleServicesFile": "./google-services.json",
			"permissions": ["INTERNET"]
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./_transparent.png"
		},
		"plugins": [
			"expo-router",
			"@react-native-firebase/app",
			"@react-native-firebase/auth",
			[
				"expo-build-properties",
				{
					"android": {
						"compileSdkVersion": 34,
						"targetSdkVersion": 34,
						"minSdkVersion": 21
					},
					"ios": {
						"useFrameworks": "static"
					}
				}
			],
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/logo_transparent.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#FFFFFF"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "9c855445-1918-4f3a-a2d7-bc0852040e63"
			}
		},
		"owner": "aakgna"
	}
}
