{
	"expo": {
		"name": "Gear",
		"slug": "gear",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "gear",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.gear.app",
			"googleServicesFile": "./GoogleService-Info.plist",
			"buildNumber": "1"
		},
		"android": {
			"versionCode": 1,
			"package": "com.gear.app",
			"googleServicesFile": "./google-services.json",
			"permissions": ["INTERNET"]
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			"@react-native-firebase/app",
			"@react-native-firebase/auth",
			[
				"expo-build-properties",
				{
					"android": {
						"compileSdkVersion": 34,
						"targetSdkVersion": 34,
						"minSdkVersion": 21
					},
					"ios": {
						"useFrameworks": "static"
					}
				}
			],
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#1e88e5"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "21c39cb8-72ed-4767-9578-30a94f165ba9"
			}
		}
	}
}
