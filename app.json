{
	"expo": {
		"name": "The Common Ground",
		"slug": "commonground",
		"version": "1.1.0",
		"orientation": "portrait",
		"icon": "./assets/images/CG3.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.aakgna.commongroundquestions",
			"googleServicesFile": "./GoogleService-Info.plist",
			"buildNumber": "2"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/CG3.png",
				"backgroundColor": "#120318"
			},
			"package": "com.aakgna.commongroundquestions",
			"googleServicesFile": "./google-services.json"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/CG3.png"
		},
		"plugins": [
			"expo-router",
			"@react-native-firebase/app",
			"@react-native-firebase/auth",
			[
				"expo-build-properties",
				{
					"ios": {
						"useFrameworks": "static"
					}
				}
			],
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/CG2.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#120318"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "21c39cb8-72ed-4767-9578-30a94f165ba9"
			}
		}
	}
}
